{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "arrow-parens": 0,
    "no-param-reassign": 0,
    "import/extensions": 0,
    "operator-linebreak": 0,
    "comma-dangle": 0
  }
}
